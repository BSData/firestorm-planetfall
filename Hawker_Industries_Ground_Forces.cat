  <rules>
    <rule id="b956-c67d-20f7-18b2" name="Hawker Industries - Artillery Attacks" hidden="false">
      <description>All Hawker Industries Artillery Attacks use the Corrosive, Barrage and Scatter MARs  with 4D6 Attack Dice.</description>
      <modifiers/>
    </rule>
    <rule id="9e545086-1c35-b094-b0de-5104d6fce761" name="Hawker Industries - Sky Drop Capability" hidden="false">
      <description>Initial Drop Site Markers are set to 5. Initial Artillery Markers are set to 5.</description>
      <modifiers/>
    </rule>
    <rule id="8467d8a0-2704-cd03-6831-4fde507b2e19" name="Hawker Industries - Tactical Bonus" hidden="false">
      <description>Hawker Industries forces have a Tactics Bonus of +2.</description>
      <modifiers/>
    </rule>
    <rule id="32d5-6a93-90db-2376" name="Hawker Industries - Logistic Bid Max" hidden="false">
      <description>Up to 5 Logistics Points in a single Bid Action.</description>
      <modifiers/>
    </rule>
  </rules>
