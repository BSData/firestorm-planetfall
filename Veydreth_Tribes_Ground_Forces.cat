    <rule id="900d4605-02a4-76ce-0978-4ba9dddc4a8e" name="Veydreth Tribes - Artillery Attacks" hidden="false">
      <description>All Veydreth Tribe Artillery Attacks use the Barrage and Scatter MARs with 5D6 Attack Dice.</description>
      <modifiers/>
    </rule>
    <rule id="7d202cfd-369d-2dd3-a1d9-dd6b456e85e2" name="Veydreth Tribes - Sky Drop Capability" hidden="false">
      <description>Initial Drop Site Markers are set to 6. Initial Artillery Markers are set to 4.</description>
      <modifiers/>
    </rule>
    <rule id="09617fe3-a35d-f5ff-9d57-25af292161a0" name="Veydreth Tribes - Tactical Bonus" hidden="false">
      <description>Veydreth Tribe forces have a Tactics Bonus of +2.</description>
      <modifiers/>
    </rule>
    <rule id="f528-af48-fe69-a7e0" name="Veydreth Tribes - Logistic Bid Max" hidden="false">
      <description>Up to 4 Logistics Points in a single Bid Action.</description>
      <modifiers/>
    </rule>
  </rules>
