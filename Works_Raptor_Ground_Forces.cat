    <rule id="900d4605-02a4-76ce-0978-4ba9dddc4a8e" name="Works_Raptor---Artillery_Attacks" hidden="false">
      <description>All Works Raptor Artillery Attacks use the Barrage, Scatter and Terror Weapon MARs with 5D6 Attack Dice.</description>
      <modifiers/>
    </rule>
    <rule id="7d202cfd-369d-2dd3-a1d9-dd6b456e85e2" name="Works_Raptor---Sky_Drop_Capability" hidden="false">
      <description>Initial Sky Drop Markers are set to 6. Initial Artillery Markers are set to 4.</description>
      <modifiers/>
    </rule>
    <rule id="09617fe3-a35d-f5ff-9d57-25af292161a0" name="Works_Raptor---Tactical_Bonus" hidden="false">
      <description>Works Raptor forces have a Tactics Bonus of +1.</description>
      <modifiers/>
    </rule>
    <rule id="fe08-ca7e-8a8c-4fc5" name="Works_Raptor---Logistical_Strength" hidden="false">
      <description>Up to 5 Logistics Points in a single Bid Action.</description>
      <modifiers/>
    </rule>
