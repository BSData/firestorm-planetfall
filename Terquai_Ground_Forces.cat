  <rules>
    <rule id="f1893d05-bdf9-7661-8870-98a4fb026a3e" name="Artillery Attacks" hidden="false">
      <description>All Terquai Artillery Attacks use the Barrage and Scatter MARs.</description>
      <modifiers/>
    </rule>
    <rule id="7495018b-c53f-093b-dea7-9aaeab0f560f" name="Artillery Dice" hidden="false">
      <description>Terquai Forces use Artillery with 4D6 Attack Dice.</description>
      <modifiers/>
    </rule>
    <rule id="20c28010-4d80-b007-bfae-ab86885b9e1b" name="Sky Drop Capability" hidden="false">
      <description>Initial Drop Site Markers are set to 4. Initial Artillery Markers are set to 6.</description>
      <modifiers/>
    </rule>
    <rule id="5b81e1a8-c1a8-fd05-316f-98a576c2ab2b" name="Tactical Bonus" hidden="false">
      <description>Terquai forces have a Tactics Bonus of +2.</description>
      <modifiers/>
    </rule>
    <rule id="6b9a-c3b6-09ae-4c68" name="Logistic Bid Max" hidden="false">
      <description>Up to 4 Logistics Points in a single Bid Action.</description>
      <modifiers/>
    </rule>
  </rules>
