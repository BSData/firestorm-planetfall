  <sharedRules>
    <rule id="d105d61e-c64a-99f7-bbc7-8a2c84a995d8" name="Bulky (2)" hidden="false" book="Core Rulebook" page="116">
      <description>These units take up 2 slots in transports.</description>
      <modifiers/>
    </rule>
    <rule id="92ad4a2c-9359-fc2a-2fd4-e8c6c01e40b7" name="Command Element (18&quot;)" hidden="false" book="Core Rulebook" page="116">
      <description>All helix squadrons in 18&quot; gain an additional morale die when testing for disorder. If this model is detroyed all squadrons in the helix must take a disorder test requiring two successes.</description>
      <modifiers/>
    </rule>
    <rule id="336a2f30-3ea0-3651-1b3c-2277057aa8c3" name="Drone" hidden="false" book="Core Rulebook" page="117">
      <description>Drones within range of a Drone Nexus become Regular and gain +1 CQB.	</description>
      <modifiers/>
    </rule>
    <rule id="fcd755dd-6502-37aa-a0f4-588907a2b5c2" name="Drone Nexus (8&quot;)" hidden="false" book="Core Rulebook" page="117">
      <description>Boosts Drones to Regular and gives them +1 CQB within 8&quot;. Each point of damage lowers the Nexus range by 4&quot;.</description>
      <modifiers/>
    </rule>
    <rule id="0654a0ed-a564-f83d-778a-cca3a6640806" name="Fearless" hidden="false" book="Core Rulebook" page="118">
      <description>Unit never takes Disordered Tests.</description>
      <modifiers/>
    </rule>
    <rule id="84c7c112-99e0-51d5-5c33-f215b285db0c" name="Flying Vehicle" hidden="false" book="Core Rulebook" page="118">
      <description>When moving at Cruising Speed the unit gains Hard Target (-1), when moving at Flat Out the unit gains Hard Target (-2).</description>
      <modifiers/>
    </rule>
    <rule id="6721-4bad-1932-7fb7" name="Ground Attack" hidden="false" book="Terran Alliance ORBAT" page="7">
      <description>This Squadron may fire its Main Ordnance while moving Flat Out provided the target squadron are Surface models â€“ These shots are considered to be Rushed.</description>
      <modifiers/>
    </rule>
    <rule id="fa16c08c-56d5-2bf8-49c0-66d4d99b48b9" name="Hard Target (-1)" hidden="false" book="Core Rulebook" page="118">
      <description>Main Ordnance is -1 to hit this unit.</description>
      <modifiers/>
    </rule>
    <rule id="924ab8ac-3403-bcea-a341-c71e58c2e6ee" name="Logistics Points" hidden="false" book="Core Rulebook" page="58">
      <description>May use up to 4 points to bid on an Opposed Tactics test.</description>
      <modifiers/>
    </rule>
    <rule id="66ea-ea88-9d43-44c6" name="Portal Capable" hidden="false" book="Aquan ORBATs" page="11">
      <description>This Squadron may make use of Temporal Portals.</description>
      <modifiers/>
    </rule>
    <rule id="4cdda248-fbaa-0c30-608f-2ab1677ea2d1" name="Portal Technology (Nabis Drones Only)" hidden="false" book="Core Rulebook" page="82">
      <description>Nabis Drones may use these portals as per Page 82 of the Core Rulebook.</description>
      <modifiers/>
    </rule>
    <rule id="5134434e-bc4c-b8b4-1c31-81873e0eef3d" name="Rear Echelon" hidden="false" book="Core Rulebook" page="120">
      <description>A model with this MAR is considered to be activated on the turn it is deployed.</description>
      <modifiers/>
    </rule>
    <rule id="41458ec0-7191-91b6-cdc1-d7ea05150f98" name="Sky Drop" hidden="false" book="Core Rulebook" page="121">
      <description>May use the Sky Drop Deployment rules on Page 79.</description>
      <modifiers/>
    </rule>
    <rule id="61ce68cc-7a35-94d5-876b-4f104b1dc706" name="Sky Drop Asset" hidden="false">
      <description>Unless otherwise noted, this model MUST deploy via Sky Drop. (Page 79 of Core Rulebook)</description>
      <modifiers/>
    </rule>
    <rule id="4fddf1fa-4639-8539-da44-031c2eaa9f9d" name="Sky Drop Nexus (Temporal Portals Only, 2)" hidden="false" book="Core Rulebook" page="121">
      <description>Places 2 Drop Site Markers for Temporal Portals in the Place Drop-Sites Segment of the Pre-Game Phase.</description>
      <modifiers/>
    </rule>
    <rule id="7490be39-1253-267f-bd1a-eb0a1c31a85a" name="Take &amp; Hold" hidden="false" book="Core Rulebook" page="121">
      <description>This unit may claim objectives.</description>
      <modifiers/>
    </rule>
    <rule id="4370584d-26b1-ffc5-66ba-bc49902295ab" name="Temporal Shift" hidden="false" book="Aquan ORBATS" page="2">
      <description>All Terquai Empire Armoured and Aerial elements may gain the Cloaked MAR at the end of their Activation provided they do not engage in CQB or perform any Main Ordnance Firing during their Activation. The Cloaked MAR lasts until the beginning of their next Activation.</description>
      <modifiers/>
    </rule>
    <rule id="18268857-4ffe-7ad7-46a3-6f965a36ae57" name="Weapon MAR:Anti-Personnel" hidden="false" book="Core Rulebook" page="116">
      <description>Weapon ignores Hard Target rules when targeting Infantry.</description>
      <modifiers/>
    </rule>
    <rule id="7c57e3a0-58e5-32bc-e5d2-f75138ca17e4" name="Weapon MAR:Nexus Designator" hidden="false" book="Core Rulebook" page="79">
      <description>Weapon is used to reduce Sky Drop Markers micro dice.</description>
      <modifiers/>
    </rule>
    <rule id="7d85f7b3-bb70-5955-2998-90d7dde550f6" name="Weapon MAR:Terror Weapon" hidden="false" book="Core Rulebook" page="122">
      <description>Disorder Tests caused by this weapon roll one less die.</description>
      <modifiers/>
    </rule>
    <rule id="05bd-2760-daf6-3a3e" name="Sectored Armour" hidden="false" book="Core Rulebook" page="121">
      <description>Model never suffers penalties to DR from Flank or Aft Main Ordnance attacks.</description>
      <modifiers/>
    </rule>
  </sharedRules>
